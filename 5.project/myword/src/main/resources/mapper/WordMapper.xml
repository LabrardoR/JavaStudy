<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.WordMapper">

    <resultMap id="BaseResultMap" type="org.head.myword.model.domain.Word">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="spell" column="spell" jdbcType="VARCHAR"/>
            <result property="library" column="library" jdbcType="VARCHAR"/>
            <result property="explanation" column="explanation" jdbcType="VARCHAR"/>
            <result property="association" column="association" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="is_delete" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,spell,library,
        explanation,association,create_time,
        update_time,is_delete
    </sql>
</mapper>
